@using  MvcHLproje.Models.Siniflar
@model List<FaturaKalem>


@{
    ViewBag.Title = "FaturaDetay";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h3>Fatura Kalemleri Sayfası</h3>
<br />

<ol class="breadcrumb mb-4">
    <li class="breadcrumb-item active"></li>
</ol>

<table class="table table-condensed">
    <tr>
        <th>Id</th>
        <th>Açıklama</th>
        <th>Miktar</th>
        <th>Birim Fiyat</th>
        <th>Tutar</th>
    </tr>

    @foreach (var x in Model)
    {
        <tr>
            <td>@x.FaturaKalemId</td>
            <td>@x.Aciklama</td>
            <td>@x.Miktar</td>
            <td>@x.BirimFiyat</td>
            <td>@x.Tutar</td>
        </tr>
    }
</table>

<a href="/Fatura/YeniKalem/" class="btn btn-primary">Yeni Kalem Girişi</a>
